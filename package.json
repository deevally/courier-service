  "homepage": "https://bitbucket.org/qutech/career-portal-backend#readme",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "argon2": "^0.25.1",
    "axios": "^0.19.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.18.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.1",
    "mongoose-paginate": "^5.0.3",
    "mongoose-unique-validator": "^2.0.3",
